                                 `(setq current-prefix-arg (list 4)))
                                ((or (stringp x)
                                     (and (listp x)
                                          (eq (car x) 'kbd)))
                                 `(lispy-unalias ,x))
                                (t x))) body)
              (insert "|")
              (when (region-active-p)
                (exchange-point-and-mark)
                (insert "~"))
              (buffer-substring-no-properties
               (point-min)
               (point-max)))
